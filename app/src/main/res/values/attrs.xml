<?xml version="1.0" encoding="utf-8"?>
<resources>
    <integer name="tag_scroll_speed">1</integer>
    <!--imageCrop Style-->
    <declare-styleable name="ClipSquareImageView">
        <attr name="ClipSquareIV_BorderDistance" format="dimension" />
        <attr name="ClipSquareIV_BorderWidth" format="dimension" />
        <attr name="ClipSquareIV_BorderColor" format="color" />
        <attr name="ClipSquareIV_OutsideColor" format="color" />
    </declare-styleable>
    <declare-styleable name="ClipZoomImageView">
        <attr name="wTh" format="float" />
    </declare-styleable>

    <declare-styleable name="CircleColorView">
        <attr name="ViewColor" format="color"/>
    </declare-styleable>

    <!-- 自定义属性 -->
    <declare-styleable name="MyTopBar">
        <attr name="leftTextColor" format="color" />
        <!-- BackGround属性可以颜色，也可以是一个资源文件 -->
        <!-- 所以BackGround是 format="reference|color"-->
        <attr name="leftBackGround" format="reference|color" />
        <attr name="leftText" format="string" />

        <attr name="title" format="string" />
        <attr name="titleTextSize" format="dimension" />
        <attr name="titleTextColor" format="color" />

        <attr name="rightTextColor" format="color" />
        <attr name="rightBackGround" format="reference|color" />
        <attr name="rightText" format="string" />
    </declare-styleable>

</resources>