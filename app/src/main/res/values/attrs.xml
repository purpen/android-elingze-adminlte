<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="bgColor_overlay">#60000000</color>
    <color name="bgColor_overlay_black">#95000000</color>
    <color name="roundColor_svprogresshuddefault">#e5e5e5</color>
    <color name="roundProgressColor_svprogresshuddefault">@android:color/black</color>
    <declare-styleable name="SVCircleProgressBar">
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="roundWidth" format="dimension"/>
        <attr name="max" format="integer"/>
        <attr name="style">
            <enum name="STROKE" value="0"/>
            <enum name="FILL" value="1"/>
        </attr>
    </declare-styleable>
    <integer name="svanimation_default_duration">300</integer>
    <integer name="tag_scroll_speed">1</integer>
    <!--imageCrop Style-->
    <declare-styleable name="ClipSquareImageView">
        <attr name="ClipSquareIV_BorderDistance" format="dimension" />
        <attr name="ClipSquareIV_BorderWidth" format="dimension" />
        <attr name="ClipSquareIV_BorderColor" format="color" />
        <attr name="ClipSquareIV_OutsideColor" format="color" />
    </declare-styleable>
    <declare-styleable name="ClipZoomImageView">
        <attr name="wTh" format="float" />
    </declare-styleable>

    <declare-styleable name="CircleColorView">
        <attr name="ViewColor" format="color"/>
    </declare-styleable>
</resources>