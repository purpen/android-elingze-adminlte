    </com.thn.erp.view.common.LinearLayoutArrowTextView>

    <include layout="@layout/include_view_layout_line" />

    <com.thn.erp.view.common.LinearLayoutArrowTextView
        android:id="@+id/layoutItemView5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/THN_color_bgColor_white">

    </com.thn.erp.view.common.LinearLayoutArrowTextView>

    <include layout="@layout/include_view_layout_line" />

    <com.thn.erp.view.common.LinearLayoutArrowTextView
        android:id="@+id/layoutItemView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/THN_color_bgColor_white">

    </com.thn.erp.view.common.LinearLayoutArrowTextView>

    <include layout="@layout/include_view_layout_line" />
</LinearLayout>
    